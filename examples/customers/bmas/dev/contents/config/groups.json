{
  "groups": {
    "admin": {
      "id": "admin",
      "name": "Admin",
      "description": "Full administrative access to all resources",
      "inherits": ["users"],
      "permissions": {
        "apps": ["*"],
        "prompts": ["*"],
        "models": ["*"],
        "adminAccess": true
      },
      "mappings": ["Admins"]
    },
    "user": {
      "id": "user",
      "name": "User",
      "description": "Standard user access to common applications and models",
      "permissions": {
        "apps": ["chat", "translator", "summarizer", "email-composer"],
        "prompts": ["*"],
        "models": ["*"],
        "adminAccess": false
      },
      "mappings": ["Users"]
    },
    "anonymous": {
      "id": "anonymous",
      "name": "Anonymous",
      "description": "Access for anonymous users (default configuration)",
      "permissions": {
        "apps": ["chat"],
        "prompts": ["*"],
        "models": ["gemini-2.0-flash"],
        "adminAccess": false
      },
      "mappings": ["anonymous", "public"]
    },
    "authenticated": {
      "id": "authenticated",
      "name": "Authenticated",
      "description": "Base permissions for all authenticated users",
      "inherits": ["anonymous"],
      "permissions": {
        "apps": ["translator", "summarizer", "email-composer", "file-analysis"],
        "prompts": ["writing"],
        "models": ["gpt-3.5-turbo", "gpt-4", "claude-4-sonnet"],
        "adminAccess": false
      },
      "mappings": []
    }
  },
  "metadata": {
    "version": "1.0.0",
    "description": "Unified group configuration with permissions and external mappings",
    "lastModified": "2025-07-20T00:00:00Z"
  }
}
