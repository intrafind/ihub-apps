# Test Environment Configuration

# Test mode settings
NODE_ENV=test
TEST_MODE=true

# API endpoints for testing
TEST_BASE_URL=http://localhost:3000
TEST_FRONTEND_URL=http://localhost:5173

# Test timeouts (in milliseconds)
TEST_API_TIMEOUT=30000
TEST_E2E_TIMEOUT=60000

# Model testing configuration
TEST_REAL_API=false
TEST_MODEL_PROVIDER=mock

# API Keys for real testing (optional - will use mocks if not provided)
# OPENAI_API_KEY=your-test-key-here
# ANTHROPIC_API_KEY=your-test-key-here
# GOOGLE_API_KEY=your-test-key-here
# MISTRAL_API_KEY=your-test-key-here

# Custom model endpoints for testing
# OPENAI_BASE_URL=https://api.openai.com/v1/chat/completions
# ANTHROPIC_BASE_URL=https://api.anthropic.com/v1/messages
# GOOGLE_BASE_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent
# MISTRAL_BASE_URL=https://api.mistral.ai/v1/chat/completions

# Test model IDs
OPENAI_MODEL_ID=gpt-4o-mini
ANTHROPIC_MODEL_ID=claude-3-haiku-20240307
GOOGLE_MODEL_ID=gemini-1.5-flash
MISTRAL_MODEL_ID=mistral-small-latest

# Authentication for testing
JWT_SECRET=test-jwt-secret-key-for-testing-only-do-not-use-in-production

# Test database/storage settings
TEST_DATA_DIR=./tests/data
TEST_UPLOADS_DIR=./tests/uploads

# Logging configuration for tests
VERBOSE_TESTS=false
LOG_LEVEL=error

# Performance testing thresholds
MAX_API_RESPONSE_TIME=5000
MAX_MODEL_RESPONSE_TIME=30000
MAX_E2E_TEST_TIME=300000

# Browser testing configuration
HEADLESS_BROWSER=true
BROWSER_TIMEOUT=30000

# Concurrent testing limits
MAX_CONCURRENT_REQUESTS=5
RATE_LIMIT_REQUESTS_PER_MINUTE=100